  script:
  - emacs --batch --no-init-file --load publish.el --funcall org-publish-all
  artifacts:
    paths:
    - public

pages:
  &lt;&lt;: *build
  only:
  - master

test:
  &lt;&lt;: *build
  except:
  - master
</pre>
